{% extends 'base.html.twig' %}

{% block title %}ResourceExtension{% endblock %}

{% block body %}
    <h1>ResourceExtension</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ resource_extension.id }}</td>
            </tr>
            <tr>
                <th>{{'label.name'|trans}}</th>
                <td>{{ resource_extension.name }}</td>
